types = {
    ## type_key_filter = colony_manager
    type[colony_manager] = {
        path = "game/setup/start"
        path = "game/in_game/setup/start"
        path = "game/main_menu/setup/start"
        path = "game/loading_screen/setup/start"
        file_extension = .txt
        unique = no
    }
}

colony_manager = {
    ## cardinality = 0..inf
    <province> = {
        tag = <country>

        category = enum[colonial_claim_category]

        reason = localisation
    }
}